model User {
  id                   String  @id @default(uuid())
  email                String  @unique
  password             String
  has_admin_privileges Boolean @default(false)
}

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
